{
  "name": "rn-ramen-example",
  "description": "Example app for rn-ramen",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
